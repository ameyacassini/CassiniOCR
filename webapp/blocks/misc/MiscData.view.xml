<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:f="sap.ui.layout.form"
		xmlns:core="sap.ui.core"
		xmlns:layout="sap.ui.layout"
		xmlns="sap.m">
	<f:Form width="100%"
		editable="false">
		<f:layout>
			<f:ResponsiveGridLayout
				labelSpanXL="12"
				labelSpanL="12"
				labelSpanM="12"
				labelSpanS="12"
				adjustLabelSpan="false"
				emptySpanXL="0"
				emptySpanL="0"
				emptySpanM="0"
				emptySpanS="0"
				columnsXL="2"
				columnsL="2"
				columnsM="2"
				singleContainerFullSize="false" />
		</f:layout>
		<f:formContainers>
			<f:FormContainer>
				<f:formElements>
					<f:FormElement label="Claim Number">
						<f:fields>
							<Input value="" editable="false" />
						</f:fields>
					</f:FormElement>
					<f:FormElement label="Agreement Number">
						<f:fields>
							<Input value="" editable="false" />
						</f:fields>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
		</f:formContainers>
	</f:Form>
</mvc:View>